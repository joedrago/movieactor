{
  "name": "movieactor",
  "version": "1.0.0",
  "description": "Movie/Actor game using IMDB and TMDB databases",
  "type": "module",
  "scripts": {
    "imdb": "node scripts/rebuild_imdb.js",
    "tmdb": "node scripts/rebuild_tmdb.js",
    "play": "node js/cli.js",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\""
  },
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "express": "^5.2.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "prettier": "^3.4.2"
  },
  "prettier": {
    "printWidth": 130,
    "tabWidth": 4,
    "trailingComma": "none",
    "semi": false
  }
}
